{
  "name": "@unique-nft/sdk",
  "version": "2.10.0",
  "packageManager": "yarn@4.1.1",
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./uniqueChain": {
      "import": "./uniqueChain.js",
      "require": "./uniqueChain.cjs",
      "types": "./uniqueChain.d.ts"
    },
    "./assetHub": {
      "import": "./assetHub.js",
      "require": "./assetHub.cjs",
      "types": "./assetHub.d.ts"
    },
    "./uniqueIndexer": {
      "import": "./uniqueIndexer.js",
      "require": "./uniqueIndexer.cjs",
      "types": "./uniqueIndexer.d.ts"
    }
  },
  "scripts": {
    "build": "tsup --clean --config configs/tsup.config.ts && yarn copy_files",
    "build:watch": "yarn copy_files && tsup --watch --config configs/tsup.config.ts",
    "copy_files": "yarn copyfiles ./package.json ./README.md ./dist",
    "test": "vitest --typecheck",
    "testrun": "vitest --typecheck --run",
    "test:nfts": "yarn vitest -c ./vitest.palletNfts.config.ts",
    "testrun:nfts": "yarn vitest --typecheck --run -c ./vitest.palletNfts.config.ts",
    "release:patch": "npm run release -- -i patch --ci",
    "release:minor": "npm run release -- -i minor --ci",
    "release:rc": "npm run release -- -i prerelease --ci",
    "release": "release-it --config configs/release-it.config.ts",
    "analyze": "tsup --metafile --clean --config configs/tsup.config.ts && esbuild-visualizer --open --metadata ./dist/metafile-esm.json",
    "schemas": "yarn tsx ./tools/copySchemas.ts"
  },
  "dependencies": {
    "@unique-nft/schemas": "^2.1.8",
    "@unique-nft/sr25519": "^0.0.2",
    "@unique-nft/utils": "^0.3.20",
    "axios": "^1.7.7",
    "qs": "^6.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/generator": "^7.25.7",
    "@babel/parser": "^7.25.8",
    "@babel/traverse": "^7.25.7",
    "@babel/types": "^7.25.8",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/qs": "^6.9.15",
    "copyfiles": "^2.4.1",
    "esbuild-plugin-ignore": "^1.1.1",
    "esbuild-visualizer": "^0.6.0",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "keywords": [
    "blockchain",
    "parachain",
    "polkadot",
    "unique network",
    "nft"
  ],
  "license": "MIT"
}
